        <!-- something in the main journeyman js file breaks the opacity transition -->
        <script src="https://dl.dropboxusercontent.com/u/96125344/journeyman.js"></script>

		<script src='https://api.tiles.mapbox.com/mapbox.js/v0.6.7/mapbox.js'></script>
		<link rel='stylesheet' href='https://api.tiles.mapbox.com/mapbox.js/v0.6.7/mapbox.css'  />
		<script src='//cdnjs.cloudflare.com/ajax/libs/underscore.js/1.5.2/underscore-min.js'></script>

		<script>
		
		// TODO:
		// - reintegrate main journeyman.js
		//		- eliminate parallaxor
		//		- figure out odd function structure??
		//		- why do posts disappear after the drawer is activated
		//		- make new markers appear with new posts
		//		- last post doesn't end the blog
		// - add option to hide location tags
		// - add date to posts
		// - trigger ease on previous ease end
		// - fix map width for large screens
		// - eliminate dependency on underscore.js
		// - eliminate cruft from grid layout and remove option
		// - write content for shutdown trip
		// - 0.5!! (share with Mike & Eric & Dad & Matthew)
		
		// - Marker cleanup:
		//		- add mapbox marker option??
		//		- replace css pin ??
		// 		- implement date handling for marker text
		// 		- fix positioning of date markers
		// 		- add circle marker (solid circle with black translucent border looks good)
		// 		- add optional pulse effect for active marker
		// 		- [fix shadow overlap]
		// - Mobile optimization: 
		// 		- add scroll effect for tablet using different event handlers
		// 		- set map not to display on mobile
		// 		- test on mobile
		// - map should only show once it has been fully loaded
		// - integrate the onscroll event with the main javascript for the theme
		// - make page degrade more elegantly when javascript is disabled (posts not invisible)
		// - 1.0!!

		var geojson = []
		var markers = [];
		
		function isNumber(n) {
			return !isNaN(parseFloat(n)) && isFinite(n);
		}
		
		$('.post').each(function() {
			var placeToMap = false;	
			var postID;
			var coordinates = [];
			var zoomLevel = 4;	
			// var mapRelatedTags = 0;
	
			$(this).find('a.tag').each( function(i,item) {
				var tagString = $(this).html();
				var googleCoordinates = [];
				
				if (tagString.indexOf("place ") >= 0){
					placeToMap = true;
					postID = $(this).parents('article').attr('data-post-id');
					googleCoordinates = tagString.replace('place ','').split(/[\s]+/);
					coordinates[0] = googleCoordinates[1];
					coordinates[1] = googleCoordinates[0];
					// $(this).remove();
					// mapRelatedTags++;
				}
		
				if (tagString.indexOf("zoom ") >= 0) {
					var zoomValue = tagString.replace('zoom ','');
					var isZoomNumber = isNumber(zoomValue);
					if(isZoomNumber) {
						zoomLevel = parseInt(zoomValue);					
					}				
					// $(this).remove();				
					// mapRelatedTags++;			
				}	
			} );
	
			// if the place & zoom tags are all the tags, remove the whole list
			/* if(mapRelatedTags === noOfTags){
				$(this).find('li.tags').remove();
			} */
	
			if(placeToMap) {
				// setup two data structures:
				// one to easily go from the geometry to the post
				// and the other to easily go from the post to the geometry
				// TODO: get post date in case we are using the date markers
				geojson.push( { 'geometry': { 'type': 'Point', 'coordinates': coordinates },
								'properties': { 'postID': postID, 'markerText': 'NYI"' } } );
				markers[postID] = {
					"geojsonIndex": geojson.length, 
					"coordinates": coordinates, 
					"zoom": zoomLevel, 
					"date": "NYI" };
			}
		});

		var main_layer = mapbox.layer().id('mas222.i1nib4id');
		var post_markers = mapbox.markers.layer()
			// Load up markers from geojson data.
			.features(geojson)
			// Define a new factory function. Takes geojson input and returns a
			// DOM element that represents the point.
			.factory(function(f) {
				// setup the marker as two nested divs since Mapbox.js uses transforms
				// to position the marker, but we also want to be able to use
				// transforms (on the inner div) to style it.
		  
				// outer div to position the marker
				var wrapper = document.createElement('div');
				wrapper.className = 'marker-wrapper + marker-' + f.properties.postID;
		
				// inner div for styling
				// placing marker text in its own span allows it to be easily hidden
				var marker = document.createElement('div');
				marker.className = 'marker {select:Marker}';
				marker.innerHTML = '<span class="marker-text" aria-hidden="true">' + f.properties.markerText + '</span>' ;
				wrapper.appendChild(marker);
			
				// inner div for shadow
				var markerShadow = document.createElement('div');
				markerShadow.className = 'marker-shadow {select:Marker}';
				wrapper.appendChild(markerShadow);
			
				return wrapper;
		  	});

		var map = mapbox.map('map', [main_layer, post_markers], null, []);
		map.centerzoom({ lat: 0, lon: 0 }, 2);

		// Array of post elements.
		var posts = document.getElementsByTagName("article");

		// Helper to set the active section.
		var setActive = function(index, ease) {
			postID = $(posts[index]).attr('data-post-id');
			
			$(".marker-wrapper").removeClass("marker-active");
			$(".marker-" + postID).addClass("marker-active");

 			$(posts[index]).addClass("visible");
 			$(posts).slice(0,index).removeClass("visible");
 			$(posts).slice(index+1).removeClass("visible"); 	   
			$(posts).addClass("scrolling");

  			// TODO: Should change this to store function in a variable instead of in the DOM
  			// TODO: Probably should not be in this fn, since it isn't about setting active post
  			// TODO: Probably should move scrollTimer function to a permanent function to avoid recreating each time
  			// TODO: When an ease is done, need to check if we're still at the right place, and if not, start a new ease
			clearTimeout($.data(this, 'scrollTimer'));
			$.data(this, 'scrollTimer', setTimeout(function() {
				$(posts).removeClass("scrolling");
		    	if (!map.ease.running() && markers[postID]) { 
		    		var dest = { "lat": markers[postID].coordinates[1], "lon": markers[postID].coordinates[0] };
		    		if (ease) {
						map.ease.location(dest).zoom(markers[postID].zoom).optimal(.6);
		  			} else {
						map.centerzoom(dest, markers[postID].zoom);
					}
				}
			}, 250));

 			return true;
		};

		// Bind to scroll events to find the active section.
		window.onscroll = _(function() {
			// IE 8
			if (window.pageYOffset === undefined) {
				var y = document.documentElement.scrollTop;
				var h = document.documentElement.clientHeight;
			} else {
				var y = window.pageYOffset;
				var h = window.innerHeight;
			}

			// If scrolled to the very top of the page set the first section active.
			if (y === 0) return setActive(0, true);

			// Otherwise, conditionally determine the extent to which page must be
			// scrolled for each section. The first section that matches the current
			// scroll position wins and exits the loop early.
			var cumulative_post_height = 60; //the height of the top of the first post at the top of the scroll
			var margin = (h * 0.4);
			var active = _(posts).any(function(el, index) {
				// (y + margin) is the pixel offset, from the top of the page, that is 1/3 of the way down the window
				// Add the height of each post, until we find the first post whose bottom has not yet cleared this point
				// This is the active post
				cumulative_post_height += $(el).outerHeight(true);
				return (cumulative_post_height > (y + margin))  ? setActive(index, true) : false;
			});

			// If no section was set active the user has scrolled past the last section.
			// Set the last section active.
			if (!active) setActive(posts.length - 1, true);
		}).debounce(10);

		/* 
		// Also bind to touch events to do the scrolling animation
		// TODO: Need to remove the fade in on mobile so that the inactive posts appear immediately
		// TODO: Need to eliminate the onscroll events on mobile so we don't flickr
		window.addEventListener('touchstart', function() { $(posts).addClass("scrolling"); } );
		window.addEventListener('touchend', function() { $(posts).removeClass("scrolling"); } );
		window.addEventListener('touchleave', function() { $(posts).removeClass("scrolling"); } );
		*/

		// Set map to first section.
		setActive(0, false);

		</script>

