            /* Journeyman additions */
            
            .marker-wrapper {
            	position: absolute;
            	z-index: 1;
            }
            
            .marker-wrapper.marker-active  {
            	z-index: 2;
            }
            
            .marker-text {
            	white-space: nowrap;
            }
            
            .marker.pin-marker {
                position: absolute;
				width: 30px;
				height: 30px;
				border: #666 solid 1px;
				border-radius: 50% 50% 50% 0;
				background: #1087bf;
				-webkit-transform: rotate(-45deg);
   				-o-transform: rotate(-45deg);
   				-ms-transform: rotate(-45deg);
     			-moz-transform: rotate(-45deg);
				transform: rotate(-45deg);
				margin: -42px 0 0 -21px;
			}
			
			.marker.pin-marker:before {
				content: '';
				position: absolute;
				width: 12px;
				height: 12px;
				margin: 9px 0 0 9px;
				background: #fff;
				border-radius: 50%;
			}
			
			.marker-active .marker.pin-marker {
				background: #3ca0d3;
				-webkit-transform: scale(1.2, 1.2) rotate(-45deg) translate(1px, -3px);
   				-o-transform: scale(1.2, 1.2) rotate(-45deg) translate(1px, -3px);
   				-ms-transform: scale(1.2, 1.2) rotate(-45deg) translate(1px, -3px);
     			-moz-transform: scale(1.2, 1.2) rotate(-45deg) translate(1px, -3px);
   				transform: scale(1.2, 1.2) rotate(-45deg) translate(1px, -3px);
			} 
			
			.pin-marker .marker-text {
				display:none;
			}
			
			.marker-shadow.pin-marker {
				content: "";
				position: absolute;
				background: #333;
				opacity: .35;
				border-radius: 50%;
				height: 18px;
				width: 18px;
				margin: -12px 0px 0px -15px;
				-webkit-transform: rotateX(55deg);
				transform: rotateX(65deg);
				z-index: -2;
			}
            
            .marker.date-marker {
            	position: absolute;
				min-width: 16px;
			 	height: 18px;
				padding: 0px 4px 0px 4px;;
				background: #1087bf;
				-webkit-border-radius: 5px;
				-moz-border-radius: 5px;
				border-radius: 5px;
				border: #3ca0d3 solid 2px;
				z-index: 1;
				font-family: 'Open Sans', sans-serif;
				font-size: 11px;
				font-weight: 600;
				line-height: 18px;
				color: white;
				text-align: center;
			}

			.marker.date-marker:after {
				content: '';
				position: absolute;
				border-style: solid;
				border-width: 6px 6px 0;
				border-color: #1087bf transparent;
				display: block;
				width: 0;
				z-index: 1;
				margin-left: -6px;
				bottom: -6px;
				left: 50%;
			}

			.marker.date-marker:before {
				content: '';
				position: absolute;
				border-style: solid;
				border-width: 7px 7px 0;
				border-color: #3ca0d3 transparent;
				display: block;
				width: 0;
				z-index: 0;
				margin-left: -7px;
				bottom: -9px;
				left: 50%;
			}
            
            .marker-active .marker.date-marker {
				background: #3ca0d3;
				border: #b7ddf3 solid 2px;
			} 
			
			.marker-active .marker.date-marker:after {
				border-color: #3ca0d3 transparent;
			}
			
			.marker-active .marker.date-marker:before {
				border-color: #b7ddf3 transparent;
            }
            
            .marker-shadow.date-marker {
				visibility: hidden;
			}

			#pane {
			  position: fixed;
			  top: 0px;
			  bottom: 0px;
			  right: 0px;
			  width: 100%;
			}

			#map  {
			  position: absolute;
			  top: 0px;
			  bottom: 0px;
			  left: 0px;
			  width: 150%;
			}

			#bg-photo  {
			  position: absolute;
			  top: 0px;
			  bottom: 0px;
			  left: 0px;
			  width: 100%;
			  background: transparent url({HeaderImage}) center center no-repeat;
			  background-size: cover;
			}
			
			body {
				background-color: transparent;
			}
			
			.post-wrapper {
   				background: none;
   			} 
			
			.post-bg {
   				background: {color:Post background};
   				background-clip: padding-box;
   				{block:IfTranslucentPosts}
   					opacity: 0.85;
   				{/block:IfTranslucentPosts}
				top: 0;
				left: 0;
				bottom: 0;
				right: 0;
				border-radius: 6px;
				position: absolute;
				z-index: -1;
   			} 
   			
   			#posts article {
   				opacity: 0;
   				-webkit-transform: scale(.95, .95);
   				-o-transform: scale(.95, .95);
   				-ms-transform: scale(.95, .95);
     			-moz-transform: scale(.95, .95);
   				transform: scale(.95, .95);
   				-webkit-transition: margin-bottom 0.2s, opacity .5s, -webkit-transform .5s;
				-o-transition: margin-bottom 0.2s, opacity .5s, -o-transform .5s;
				-ms-transition: margin-bottom 0.2s, opacity .5s, -ms-transform .5s; 
				-moz-transition: margin-bottom 0.2s, opacity .5s, -moz-transform .5s; 
				transition: margin-bottom 0.2s, opacity .5s, transform .5s; 
			}
			
			#posts article.scrolling {
   				opacity: .25;
			}
			
			#posts article.visible {
   				opacity: 1;
   				-webkit-transform: scale(1, 1);
   				-o-transform: scale(1, 1);
   				-ms-transform: scale(1, 1);
     			-moz-transform: scale(1, 1);
   				transform: scale(1, 1);
			}
  	
            /* Custom CSS */
