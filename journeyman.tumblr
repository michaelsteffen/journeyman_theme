                .nav-menu .pop-menu ul {
                    background: {BackgroundColor};
                }

                .nav-menu.active .selector .bar-icon,
                .below-header .selector .bar-icon {
                    background: {AccentColor};
                }

                .nav-menu .pop-menu ul li a,
